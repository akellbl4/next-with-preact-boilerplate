{
  "preset": "ts-jest",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "setupFiles": ["<rootDir>/jest.setup.ts"],
  "moduleDirectories": ["src", "node_modules"],
  "moduleFileExtensions": ["ts", "tsx", "js", "json"],
  "moduleNameMapper": {
    "\\.css": "identity-obj-proxy",
    "assets/(.*)": "identity-obj-proxy"
  },
  "globals": {
    "ts-jest": {
      "babelConfig": {
        "presets": ["next/babel"]
      }
    }
  }
}
